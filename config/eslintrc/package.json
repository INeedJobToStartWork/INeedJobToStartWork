{
	"name": "@ineedj/eslintrc",
	"version": "1.2.4",
	"description": "INEEDJ config for Eslint",
	"keywords": [
		"eslintrc",
		"strict",
		"hardcore",
		"ineedj",
		"ineedjobtostartwork"
	],
	"homepage": "https://github.com/INeedJobToStartWork/INeedJobToStartWork",
	"bugs": "https://github.com/INeedJobToStartWork/INeedJobToStartWork/issues/new/choose",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/INeedJobToStartWork/INeedJobToStartWork",
		"directory": "config/eslintrc"
	},
	"license": "MIT",
	"author": "ineedjobtostartwork",
	"type": "module",
	"exports": {
		".": {
			"types": {
				"import": "./index.d.ts",
				"require": "./index.d.cts"
			},
			"import": "./index.js",
			"require": "./index.cjs",
			"*": "./*"
		}
	},
	"scripts": {
		"build": "pnpm tsup-node --config ./config/tsuprc/tsup.prod.ts ",
		"build:npm": "pnpm npm:prepack && pnpm tsup-node --config ./config/tsuprc/tsup.prod.ts && pnpm npm:postpack",
		"dev": "pnpm tsup-node --config ./config/tsuprc/tsup.dev.ts",
		"dts": "pnpm dts-bundle-generator --config ./config/dts-bundle-generator/dts-bundle-generator.cjs",
		"format": "pnpm prettier . --write && pnpm prettier . --check",
		"lint": "pnpm eslint .",
		"lint:fix": "pnpm eslint . --fix",
		"npm:postpack": "clean-package restore",
		"npm:prepack": "clean-package",
		"prepublishOnly": "pnpm build:npm",
		"start": "pnpm exec eslint-config-inspector"
	},
	"dependencies": {
		"@next/eslint-plugin-next": "15.3.0",
		"@rushstack/eslint-patch": "1.11.0",
		"@stylistic/eslint-plugin": "4.2.0",
		"@typescript-eslint/eslint-plugin": "8.29.1",
		"confusing-browser-globals": "1.0.11",
		"eslint": "9.24.0",
		"eslint-config-flat-gitignore": "2.1.0",
		"eslint-flat-config-utils": "2.0.1",
		"eslint-import-resolver-node": "0.3.9",
		"eslint-plugin-array-func": "5.0.2",
		"eslint-plugin-ban": "2.0.0",
		"eslint-plugin-command": "3.2.0",
		"eslint-plugin-compat": "6.0.2",
		"eslint-plugin-css-modules": "2.12.0",
		"eslint-plugin-deprecation": "3.0.0",
		"eslint-plugin-header": "3.1.1",
		"eslint-plugin-import": "2.31.0",
		"eslint-plugin-jsonc": "2.20.0",
		"eslint-plugin-jsx-a11y": "6.10.2",
		"eslint-plugin-markdownlint": "0.6.0",
		"eslint-plugin-mdx": "3.4.0",
		"eslint-plugin-n": "17.17.0",
		"eslint-plugin-no-explicit-type-exports": "0.12.1",
		"eslint-plugin-no-unsanitized": "4.1.2",
		"eslint-plugin-only-error": "1.0.2",
		"eslint-plugin-only-warn": "1.1.0",
		"eslint-plugin-optimize-regex": "1.2.1",
		"eslint-plugin-perfectionist": "4.11.0",
		"eslint-plugin-pii": "1.0.2",
		"eslint-plugin-promise": "7.2.1",
		"eslint-plugin-react": "7.37.5",
		"eslint-plugin-react-form-fields": "1.2.22",
		"eslint-plugin-react-hook-form": "0.3.0",
		"eslint-plugin-react-hooks": "5.2.0",
		"eslint-plugin-react-perf": "3.3.3",
		"eslint-plugin-react-prefer-function-component": "4.0.1",
		"eslint-plugin-react-refresh": "0.4.19",
		"eslint-plugin-readable-tailwind": "2.0.0",
		"eslint-plugin-security": "3.0.1",
		"eslint-plugin-sonarjs": "3.0.2",
		"eslint-plugin-ssr-friendly": "1.3.0",
		"eslint-plugin-storybook": "0.12.0",
		"eslint-plugin-tailwindcss": "3.18.0",
		"eslint-plugin-toml": "0.12.0",
		"eslint-plugin-tsdoc": "0.4.0",
		"eslint-plugin-turbo": "2.5.0",
		"eslint-plugin-typescript-compat": "1.0.2",
		"eslint-plugin-typescript-sort-keys": "3.3.0",
		"eslint-plugin-unicorn": "58.0.0",
		"eslint-plugin-unused-imports": "4.1.4",
		"eslint-plugin-validate-jsx-nesting": "0.1.1",
		"eslint-plugin-vitest": "0.5.4",
		"eslint-plugin-yml": "1.17.0",
		"globals": "16.0.0",
		"local-pkg": "1.1.1"
	},
	"devDependencies": {
		"@eslint/config-inspector": "^1.0.2",
		"@ineedj/prettierrc": "^2.0.0",
		"@ineedj/tsconfig": "workspace:*",
		"@ineedj/tsuprc": "workspace:*",
		"@types/eslint-plugin-security": "^3.0.0",
		"clean-package": "^2.2.0",
		"dts-bundle-generator": "^9.5.1",
		"esbuild-plugin-copy": "^2.1.1",
		"eslint-typegen": "^0.3.2",
		"tsup": "^8.1.0",
		"typescript": "^5.5.3"
	},
	"publishConfig": {
		"directory": "dist",
		"provenance": true
	}
}
