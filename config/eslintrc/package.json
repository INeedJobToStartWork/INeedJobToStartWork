{
	"name": "@ineedj/eslintrc",
	"version": "0.10.0",
	"description": "INEEDJ config for Eslint",
	"keywords": [
		"eslintrc",
		"strict",
		"hardcore",
		"ineedj",
		"ineedjobtostartwork"
	],
	"homepage": "https://github.com/INeedJobToStartWork/INeedJobToStartWork",
	"bugs": "https://github.com/INeedJobToStartWork/INeedJobToStartWork/issues/new/choose",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/INeedJobToStartWork/INeedJobToStartWork",
		"directory": "config/eslintrc"
	},
	"license": "MIT",
	"author": "ineedjobtostartwork",
	"type": "module",
	"exports": {
		".": {
			"types": {
				"import": "./dist/index.d.ts",
				"require": "./dist/index.d.cts"
			},
			"import": "./dist/index.js",
			"require": "./dist/index.cjs",
			"*": "./dist/*"
		}
	},
	"scripts": {
		"build": "pnpm tsup-node --config ./config/tsuprc/tsup.prod.ts ",
		"build:npm": "pnpm npm:prepack && pnpm tsup-node --config ./config/tsuprc/tsup.prod.ts && pnpm npm:postpack",
		"dev": "pnpm tsup-node --config ./config/tsuprc/tsup.dev.ts",
		"format": "pnpm prettier . --write && pnpm prettier . --check",
		"lint": "pnpm eslint .",
		"lint:fix": "pnpm eslint . --fix",
		"npm:postpack": "clean-package restore",
		"npm:prepack": "clean-package",
		"prepublishOnly": "pnpm build:npm",
		"start": "pnpm exec eslint-config-inspector"
	},
	"devDependencies": {
		"@eslint/config-inspector": "^0.5.0",
		"@ineedj/prettierrc": "^2.0.0",
		"@ineedj/tsconfig": "workspace:*",
		"@ineedj/tsuprc": "workspace:*",
		"@types/eslint-plugin-security": "^3.0.0",
		"clean-package": "^2.2.0",
		"dts-bundle-generator": "^9.5.1",
		"esbuild-plugin-copy": "^2.1.1",
		"tsup": "^8.1.0",
		"typescript": "^5.5.3"
	},
	"peerDependencies": {
		"@next/eslint-plugin-next": "~14.2.5",
		"@rushstack/eslint-patch": "~1.10.3",
		"@stylistic/eslint-plugin": "~2.3.0",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"confusing-browser-globals": "~1.0.11",
		"eslint": "~8.57.0",
		"eslint-config-flat-gitignore": "^0.1.8",
		"eslint-import-resolver-node": "~0.3.9",
		"eslint-plugin-compat": "~6.0.0",
		"eslint-plugin-deprecation": "~3.0.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-json": "~4.0.0",
		"eslint-plugin-jsx-a11y": "~6.9.0",
		"eslint-plugin-no-explicit-type-exports": "~0.12.1",
		"eslint-plugin-no-unsanitized": "~4.0.2",
		"eslint-plugin-node": "~11.1.0",
		"eslint-plugin-optimize-regex": "~1.2.1",
		"eslint-plugin-perfectionist": "~2.11.0",
		"eslint-plugin-pii": "~1.0.2",
		"eslint-plugin-promise": "~6.5.1",
		"eslint-plugin-react": "~7.35.0",
		"eslint-plugin-react-form-fields": "~1.2.22",
		"eslint-plugin-react-hook-form": "~0.3.0",
		"eslint-plugin-react-hooks": "~4.6.2",
		"eslint-plugin-react-perf": "~3.3.2",
		"eslint-plugin-react-prefer-function-component": "~3.3.0",
		"eslint-plugin-react-refresh": "~0.4.9",
		"eslint-plugin-security": "~3.0.1",
		"eslint-plugin-sonarjs": "~1.0.3",
		"eslint-plugin-ssr-friendly": "~1.3.0",
		"eslint-plugin-storybook": "~0.8.0",
		"eslint-plugin-tailwindcss": "~3.17.4",
		"eslint-plugin-toml": "~0.11.1",
		"eslint-plugin-tsdoc": "~0.3.0",
		"eslint-plugin-typescript-compat": "~1.0.2",
		"eslint-plugin-unicorn": "~54.0.0",
		"eslint-plugin-unused-imports": "~3.2.0",
		"eslint-plugin-validate-jsx-nesting": "~0.1.1",
		"eslint-plugin-yml": "~1.14.0",
		"globals": "~15.8.0",
		"local-pkg": "^0.5.0"
	},
	"publishConfig": {
		"directory": "dist",
		"provenance": true
	}
}
